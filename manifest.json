{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "system.cpu",
    "system.memory",
    "system.storage",
    "system.display"
  ],
  "externally_connectable": {
    "matches": ["http://localhost:3000/*"]
  }
}
